<article class="prose">
  <div>
    <h1>
      <%= page.title %>
    </h1>
  </div>
  <div class="my-2 text-overlay1 text-sm">
    <span><%- remapDate(date(page.date)) %>,</span>
    <span><%- date(page.date, "YYYY" ) %></span>
  </div>
  <%- toc(page.content, {list_number: false}) %>
    <div class="articleLink">
      <%- page.content %>
    </div>
</article>
